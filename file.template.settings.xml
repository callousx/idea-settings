<application>
  <component name="ExportableFileTemplateSettings">
    <default_templates>
      <template name="Component in folder.js" file-name="${NAME}/${NAME}" reformat="true" live-template-enabled="false">
        <template name="Component in folder.js.child.0.js" file-name="${NAME}/index" reformat="true" live-template-enabled="false" />
      </template>
      <template name="Component in folder + .styles.jsx + .stories.js.js" file-name="${NAME}/${NAME}" reformat="true" live-template-enabled="false">
        <template name="Component in folder + .styles.jsx + .stories.js.js.child.0.jsx" file-name="${NAME}/${NAME}.styles" reformat="true" live-template-enabled="false" />
        <template name="Component in folder + .styles.jsx + .stories.js.js.child.1.js" reformat="true" live-template-enabled="false" />
        <template name="Component in folder + .styles.jsx + .stories.js.js.child.2.js" reformat="true" live-template-enabled="false" />
      </template>
      <template name="Component in folder + .styles.jsx + .stories.js.js.child.0.jsx" file-name="${NAME}/${NAME}.styles" reformat="true" live-template-enabled="false" />
      <template name="Component in folder.js.child.0.js" file-name="${NAME}/index" reformat="true" live-template-enabled="false" />
    </default_templates>
  </component>
</application>