<application>
  <component name="PostfixTemplates">
    <template id="co@userDefined" key=".co" provider="builtin.js" language-level="ES6" topmost="true">
      <conditions />
      <template name="fakeKey" value="const $EXPR$ = $END$" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="is@userDefined" key=".is" provider="builtin.js" language-level="ES6" topmost="false">
      <conditions />
      <template name="fakeKey" value="{$END$ &amp;&amp; ($EXPR$)}" toReformat="true" toShortenFQNames="true" />
    </template>
  </component>
</application>